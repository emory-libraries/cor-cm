efs_mounts:
  - path: /mnt/test_efs
    dns_name: fs-97466374.efs.us-east-1.amazonaws.com
  - path: /mnt/prod_efs
    dns_name: fs-a1416442.efs.us-east-1.amazonaws.com
rds_host: dlp-tst-rdsmys-sz
ec2_instance_type: t3.large
git_repo_branch: v0.4.7

symblink_dirs:
  - src: "/mnt/test_efs/uploads"
    path: "{{ capistrano_base }}/uploads"

__dlp_curate_dotenv:
  fedora_url: "http://fedora-cor-test.library.emory.edu/fcrepo/rest"
  solr_url: "http://solr-cor-test.library.emory.edu/solr/curate_collection"
  import_path: "/mnt/prod_efs/Collections/dmfiles/MARBL/Manuscripts/MSS_1218_Langmuir/"

dlp_curate_mysql_db:
  name: "{{ vault_curate.test.db.name }}"
  user: "{{ vault_curate.test.db.user }}"
  password: "{{ vault_curate.test.db.pass }}"

sp_entity_id: "https://curate.library.emory.edu"
idp_name: "prod"

ec2_security_groups:
  - "{{ sg_pre }}-tp-apache-ssh"
  - "{{ sg_pre }}-t-nfs"
  - "{{ sg_pre }}-t-mysql"
  - "{{ sg_pre }}-t-solr"
  - "{{ sg_pre }}-t-redis"

elb_targets:
  - target_group_name: curate-test-443
