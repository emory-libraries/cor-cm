git_repo_url: "https://github.com/emory-libraries/dlp-curate"
git_repo_branch: "master"
enable_ssl: "yes"
ec2_efs: fs-30e25829

dlp_curate_dotenv:
  admin_password: "{{ vault_project_default_admin_password }}"
  contact_us_email: "test@emory.edu"
  cache_path: "/opt/uploads/cache"
  upload_path: "/opt/uploads"
  working_path: "/opt/uploads"
  derivatives_path: "/opt/derivatives"
  database_auth: false
  database_adapter: "mysql2"
  database_pool: 5
  database_name: "{{ vault_test_rails_db_name }}"
  database_username: "{{ vault_test_rails_db_user }}"
  database_host: "{{ rds_endpoint }}"
  database_password: "{{ vault_test_rails_db_pass }}"
  geonames_username: "{{ vault_geonames_username }}"
  project_name: "dlp-curate"
  sidekiq_workers: 5
  fedora_url: "http://fedora-cor-test.library.emory.edu/fcrepo/rest"
  fedora_user: "{{ vault_fedora_admin_user }}"
  fedora_password: "{{ vault_fedora_admin_password }}"
  solr_url: "https://util-cor-test.library.emory.edu/solr/curatecore"
  redis_host: localhost
  redis_port: 6379

dlp_curate_mysql_db:
  name: "{{ vault_test_rails_db_name }}"
  user: "{{ vault_test_rails_db_user }}"
  password: "{{ vault_test_rails_db_pass }}"

sp_entity_id: "https://curate.library.emory.edu"
idp_name: "prod"
get_metadata:
  download: no
