rds_host: dlp-tst-rdsmys-sz
efs_mount_points:
  - path: /mnt/test_efs
    dns_name: fs-97466374.efs.us-east-1.amazonaws.com
elb_targets:
  - target_group_name: lux-test-443
dlp_lux_mysql_db:
  name: '{{ vault_lux.test.db.name }}'
  user: '{{ vault_lux.test.db.user }}'
  password: '{{ vault_lux.test.db.pass }}'

sp_entity_id: https://digital.library.emory.edu

#### Solr
solr_cores:
  - ident: lux_core
    type: hyrax

ec2_collect_list:
  - file_path: "/opt/data/solr/logs/solr*"
    log_group_name: solr
    log_stream_name: "{{ solr_alias }}"

ec2_security_groups:
  - "{{ sg_pre }}-tp-apache-ssh"
  - "{{ sg_pre }}-t-nfs"
  - "{{ sg_pre }}-t-mysql"
  - "{{ sg_pre }}-t-solr"
  - "{{ sg_pre }}-t-redis"

solr_collections: false