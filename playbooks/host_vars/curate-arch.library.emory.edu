efs_mount_points:
  - path: /mnt/arch_efs
    dns_name: fs-7bda9999.efs.us-east-1.amazonaws.com
  - path: /mnt/import_efs
    dns_name: fs-a1416442.efs.us-east-1.amazonaws.com
rds_host: dlp-tst-rdsmys-sz

symblink_dirs:
  - src: /mnt/arch_efs/uploads
    path: '{{ capistrano_base }}/uploads'

__dlp_curate_dotenv:
  branding_path: /mnt/arch_efs/uploads/dlp-curate/branding
  iiif_cookie_salt_1: '{{ vault_dlp.iiif.arch.cookie.salt_1 }}'
  iiif_cookie_salt_2: '{{ vault_dlp.iiif.arch.cookie.salt_2 }}'
  lux_base_url: digital-arch.library.emory.edu
  fedora_url: http://fedora-cor-arch.library.emory.edu/fcrepo/rest
  solr_url: http://solr-cor-arch-1.library.emory.edu/solr/curate_collection
  proxied_iiif_server_url: http://iiif-cor-arch.library.emory.edu/cantaloupe/iiif/2/
  redis_database: 2

dlp_curate_mysql_db:
  name: '{{ vault_curate.arch.db.name  | d () }}'
  user: '{{ vault_curate.arch.db.user | d () }}'
  password: '{{ vault_curate.arch.db.pass | d() }}'

sp_entity_id: https://curate.library.emory.edu
idp_name: prod

ec2_security_groups:
  - '{{ sg_pre }}-tp-ssh'
  - '{{ sg_pre }}-tp-apache'
  - '{{ sg_pre }}-t-nfs'
  - '{{ sg_pre }}-p-nfs'
  - '{{ sg_pre }}-t-mysql'
  - '{{ sg_pre }}-t-solr'
  - '{{ sg_pre }}-t-redis'
  - '{{ sg_pre }}-a-http-to-fedora'
  - '{{ sg_pre }}-a-http-to-solr'
  - '{{ sg_pre }}-a-http-to-iiif'

elb_targets:
  - target_group_name: curate-arch-443

cap_branding_symlink_path: /mnt/arch_efs/uploads/dlp-curate/branding